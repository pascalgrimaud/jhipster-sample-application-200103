<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Material.
    -->
    <changeSet id="20200103074355-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="material_id"
                                 baseTableName="material_origin"
                                 constraintName="fk_material_origin_material_id"
                                 referencedColumnNames="id"
                                 referencedTableName="material"/>

        <addForeignKeyConstraint baseColumnNames="origin_id"
                                 baseTableName="material_origin"
                                 constraintName="fk_material_origin_origin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="material_origin"/>
        
    </changeSet>
</databaseChangeLog>
